{
    "name": "starter-kit",
    "version": "1.0.0",
    "description": "A JavaScript development project starter kit tailored to my own personal preferences.",
    "scripts": {
        "build": "webpack",
        "lint": "esw src --color",
        "lint:watch": "npm run lint -- --watch",
        "open": "webpack-dev-server --open",
        "postinstall": "npm audit",
        "start": "npm-run-all --parallel open lint:watch",
        "start-browsersync": "browser-sync start --server --port=3000 --index=index.html --files='src/*'"
    },
    "devDependencies": {
        "@babel/core": "^7.8.3",
        "@babel/preset-env": "^7.8.3",
        "@babel/preset-react": "^7.8.3",
        "babel-loader": "^8.0.6",
        "browser-sync": "^2.26.7",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.4.2",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-plugin-import": "^2.20.0",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.18.0",
        "eslint-plugin-react-hooks": "^1.7.0",
        "eslint-watch": "^6.0.1",
        "file-loader": "^5.0.2",
        "html-webpack-plugin": "^3.2.0",
        "live-server": "^1.2.1",
        "npm-run-all": "^4.1.5",
        "style-loader": "^1.1.3",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1"
    },
    "author": "Josh Loehr",
    "license": "MIT"
}
