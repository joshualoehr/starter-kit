{
    "name": "starter-kit",
    "version": "1.0.0",
    "description": "A JavaScript development project starter kit tailored to my own personal preferences.",
    "scripts": {
        "build": "webpack",
        "generate-mock-data": "babel-node --presets @babel/preset-env scripts/generateMockData.js",
        "lint": "esw src --color",
        "lint:watch": "npm run lint -- --watch",
        "postinstall": "npm audit",
        "start": "npm-run-all --parallel start-app start-mockapi lint:watch",
        "start-app": "webpack-dev-server --open --port 3000",
        "start-browsersync": "browser-sync start --server --port=3000 --index=index.html --files='src/*'",
        "prestart-mockapi": "npm run generate-mock-data",
        "start-mockapi": "json-server --watch src/api/db.json --port 3001"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.3",
        "@babel/core": "^7.8.3",
        "@babel/node": "^7.8.3",
        "@babel/preset-env": "^7.8.3",
        "@babel/preset-react": "^7.8.3",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "browser-sync": "^2.27.10",
        "chalk": "^3.0.0",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.4.2",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.18.0",
        "eslint-plugin-react-hooks": "^1.7.0",
        "eslint-watch": "^6.0.1",
        "faker": "^4.1.0",
        "file-loader": "^5.0.2",
        "html-webpack-plugin": "^3.2.0",
        "json-schema-faker": "^0.5.0-rc23",
        "json-server": "^0.15.1",
        "live-server": "^1.2.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "style-loader": "^1.1.3",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.10.1"
    },
    "author": "Josh Loehr",
    "license": "MIT",
    "dependencies": {
        "react": "^16.12.0",
        "react-dom": "^16.12.0"
    }
}
